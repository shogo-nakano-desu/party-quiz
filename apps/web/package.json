{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "description": "Party quiz app web front end application",
  "author": "shogo-nakano-desu",
  "scripts": {
    "build": "yarn build:gql && yarn build:theme && yarn build:next",
    "build:gql": "graphql-codegen --config graphql/codegen.yml",
    "build:next": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "env-cmd -f ../../.env cross-env NODE_ENV=test jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graphql": "15.8.0"
  },
  "devDependencies": {
    "env-cmd": "10.1.0",
    "eslint": "8.26.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-jest": "25.3.4",
    "jest": "29.2.1",
    "typescript": "4.5.4"
  }
}
